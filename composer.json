{
    "name": "genealabs/nova-passport-manager",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0",
        "laravel/nova": "^3.0|^4.0",
        "laravel/passport": "*"
    },
    "require-dev": {
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\NovaPassportManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\NovaPassportManager\\Providers\\Service"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/thanks": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
